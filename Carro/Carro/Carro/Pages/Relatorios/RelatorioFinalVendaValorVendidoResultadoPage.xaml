<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Carro.Pages.Relatorios.RelatorioFinalVendaValorVendidoResultadoPage"
             BackgroundColor="#FFFFF7"
             Title="ValorVendido">
    
    <StackLayout>
        <Label
	        Text="Voce realizou um total de :"/>

        <Label
	        Text="{Binding quantidadeVendas}"/>

        <Label
	        Text="vendas, totallizando:"/>

        <Label
	        Text="{Binding valorVendido}"/>
    </StackLayout>
</ContentPage>